{
  "name": "@9renpoto/app",
  "version": "0.0.1",
  "dependencies": {
    "react": "16.6.0",
    "react-native": "0.55.4"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "4.6.0",
    "@9renpoto/eslint-config-react": "4.6.0",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "4.0.1",
    "codecov": "3.1.0",
    "eslint": "5.8.0",
    "eslint-plugin-jest": "21.26.2",
    "flow-bin": "0.85.0",
    "flow-typed": "2.5.1",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "npm-run-all": "4.1.3",
    "prettier-eslint-cli": "4.7.1",
    "react-test-renderer": "16.6.0",
    "snyk": "1.105.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ]
  },
  "private": true,
  "scripts": {
    "coverage": "codecov -f ./coverage/lcov.info -t $CODECOV_TOKEN",
    "precommit": "lint-staged",
    "flow": "flow-typed install -s && flow",
    "lint:es": "eslint . --ext=js,jsx",
    "lint": "run-p lint:*",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "snyk": true
}
