{
  "name": "@9renpoto/app",
  "version": "0.0.1",
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.52.0",
    "snyk": "^1.68.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "^0.6.0",
    "@9renpoto/eslint-config-react": "^0.2.0",
    "babel-jest": "22.1.0",
    "babel-preset-react-native": "4.0.0",
    "codecov": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-plugin-jest": "^21.7.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jest": "22.1.1",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ]
  },
  "private": true,
  "scripts": {
    "coverage": "codecov -f ./coverage/lcov.info -t $CODECOV_TOKEN",
    "precommit": "lint-staged",
    "flow": "flow-typed install -s && flow",
    "lint:es": "eslint . --ext=js,jsx",
    "lint": "run-p lint:*",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
}
